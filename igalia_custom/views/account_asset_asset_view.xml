<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
    <record id="view_account_asset_search_inh_igaliacustom" model="ir.ui.view">
        <field name="name">view.account.asset.search.inh.igaliacustom</field>
        <field name="model">account.asset.asset</field>
        <field name="inherit_id" ref="account_asset.view_account_asset_search"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
              <field name="state" />
              <field name="active" />
              <group expand="0" string="Group By">
                  <filter string="State" domain="[]" context="{'group_by':'state'}"/>
                  <filter string="Active" domain="[]" context="{'group_by':'active'}"/>
              </group>
            </field>
        </field>
    </record>
    <record id="view_account_asset_asset_tree_inh_igaliacustom" model="ir.ui.view">
        <field name="name">view.account.asset.asset.tree.inh.igaliacustom</field>
        <field name="model">account.asset.asset</field>
        <field name="inherit_id" ref="account_asset.view_account_asset_asset_tree"/>
        <field name="arch" type="xml">
            <field name="category_id" position="after">
               <field name="active"/>
            </field>
        </field>
    </record>
</data>
</openerp>
