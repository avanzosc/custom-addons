<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_picking_form_inh_astikar" model="ir.ui.view">
            <field name="name">view.picking.form.inh.astikar</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='move_lines']" position="attributes">
                    <attribute name="context">{'default_date': date, 'default_date_expected': min_date, 'default_picking_id': active_id, 'default_location_id': location_id, 'default_location_dest_id': location_dest_id}</attribute>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
