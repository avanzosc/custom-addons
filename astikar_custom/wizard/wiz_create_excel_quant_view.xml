<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="wiz_create_excel_quant_form">
            <field name="name">wiz.create.excel.quant.form</field>
            <field name="model">wiz.create.excel.quant</field>
            <field name="arch" type="xml">
                <form string="Crear excel con informacion de quants" version="7.0">
                    <footer>
                        <button name="button_create_excel" type="object"
                            string="Generate excel" class="oe_highlight" />
